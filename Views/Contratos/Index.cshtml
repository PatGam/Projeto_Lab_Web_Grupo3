@model IEnumerable<Projeto_Lab_Web_Grupo3.Models.Contratos>


@{
    Layout = "_LayoutAdmin";
    ViewData["Title"] = "Index";
}



<!-- container section start -->
<section id="container" class="">
    <section id="main-content">
        <section class="wrapper">



            <div class="container row p-0 m-0">
                <div class="col-6">
                    <h2 class="text-info">Contratos</h2>
                </div>
                <div class="col-3 offset-3">
                </div>
                <div class="col-12 border p-3">
                    <table id="DT_load_Contrato" class="table table-striped table-bordered" style="width:100%">
                        <thead>
                            <tr>
                                <th>Data Inicio</th>
                                <th>Data Fim</th>
                                <th>Valor</th>
                                <th>Telefone</th>
                                <th></th>
                            </tr>
                        </thead>
                    </table>
                </div>
            </div>
        </section>
    </section>
</section>
@section Scripts {
    <script src="~/js/Contratos.js"></script>
}