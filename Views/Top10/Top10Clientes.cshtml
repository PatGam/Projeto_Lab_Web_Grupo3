@model Top10ViewModel

@{
    ViewData["Title"] = "Top 10 Clientes mais Antigos";
    Layout = "_LayoutAdmin";
}

<body>
    <!-- container section start -->
    <section id="container" class="">
        <section id="main-content">
            <section class="wrapper">
                <div class="row">
                    <div class="col-lg-12">
                        <h3 class="page-header"><i class="fa fa-file-text-o"></i>Top 10 Clientes por Distrito</h3>
                        <ol class="breadcrumb">
                            <li><i class="fa fa-home"></i><a href="index.html">Home</a></li>
                            <li><i class="icon_document_alt"></i>Gerir Clientes</li>
                            <li><i class="fa fa-file-text-o"></i>Consultar</li>
                        </ol>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        <section class="panel">
                            <header class="panel-heading">
                                @ViewBag.Distrito1
                            </header>
                            <table class="table table-striped table-advance table-hover">
                                <thead>
                                    <tr>
                                        <th>
                                            Nome
                                        </th>
                                        <th>
                                            Faturação
                                        </th>
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model.clientesAveiro)
                                    {
                                        <tr>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.ClienteNome)
                                            </td>
                                            <td>
                                                @Html.DisplayFor(modelItem => item.Lucro)
                                            </td>
                                            <td>
                                                <a asp-area="" asp-controller="Utilizadores" asp-action="Details" asp-route-id="@item.UtilizadorId">Detalhes |</a>
                                            </td>

                                        </tr>
                                    }

                                </tbody>
                            </table>

                        </section>
                    </div>
                </div>
