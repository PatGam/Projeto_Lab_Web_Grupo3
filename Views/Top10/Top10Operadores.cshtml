                            <header class="panel-heading tab-bg-primary ">
                                <ul class="nav nav-tabs">
                                    <li class="dropdown">
                                        <a href="javascript:;" class="dropdown-toggle" data-toggle="dropdown">Norte<b class="caret" style="color:white"></b></a>
                                        <ul class="dropdown-menu">
                                            <li class=""><a data-toggle="tab" href="#viana">Viana do Castelo</a></li>
                                            <li class=""><a data-toggle="tab" href="#braga">Braga</a></li>
                                            <li class=""><a data-toggle="tab" href="#porto">Porto</a></li>
                                            <li class=""><a data-toggle="tab" href="#vilareal">Vila Real</a></li>
                                            <li class=""><a data-toggle="tab" href="#braganca">Bragança</a></li>
                                        </ul>
                                    </li>
                                    <li class="dropdown">
                                        <a href="javascript:;" class="dropdown-toggle" data-toggle="dropdown">Centro<b class="caret"></b></a>
                                        <ul class="dropdown-menu">
                                            <li><a data-toggle="tab" href="#coimbra">Coimbra</a></li>
                                            <li><a data-toggle="tab" href="#aveiro">Aveiro</a></li>
                                            <li><a data-toggle="tab" href="#viseu">Viseu</a></li>
                                            <li><a data-toggle="tab" href="#leiria">Leiria</a></li>
                                            <li><a data-toggle="tab" href="#castelobranco">Castelo Branco</a></li>
                                            <li><a data-toggle="tab" href="#guarda">Guarda</a></li>
                                        </ul>
                                    </li>
                                    <li class="dropdown">
                                        <a href="javascript:;" class="dropdown-toggle" data-toggle="dropdown">Sul<b class="caret"></b></a>
                                        <ul class="dropdown-menu">
                                            <li><a data-toggle="tab" href="#lisboa">Lisboa</a></li>
                                            <li><a data-toggle="tab" href="#setubal">Setúbal</a></li>
                                            <li><a data-toggle="tab" href="#santarem">Santarém</a></li>
                                            <li><a data-toggle="tab" href="#portalegre">Portalegre</a></li>
                                            <li><a data-toggle="tab" href="#evora">Évora</a></li>
                                            <li><a data-toggle="tab" href="#beja">Beja</a></li>
                                            <li><a data-toggle="tab" href="#faro">Faro</a></li>
                                        </ul>
                                    </li>
                                    <li class="dropdown">
                                        <a href="javascript:;" class="dropdown-toggle" data-toggle="dropdown">Ilhas<b class="caret"></b></a>
