                                            <li><a data-toggle="tab" href="#acores">Açores</a></li>
                                            <li><a data-toggle="tab" href="#madeira">Madeira</a></li>

                                        </ul>
                                    </li>
                                </ul>

                            </header>
                        </section>

                        <div class="tab-content">
                            <div id="aveiro" class="tab-pane">
                                <section class="panel">
                                    <header class="panel-heading">
                                        @ViewBag.Aveiro
                                    </header>

                                    @{int pos = 1;
                                        if (Model.operadoresAveiro.Count() == 0)
                                        {
                                            <p>O distrito não tem operadores registados.</p>

                                        }
                                        else
                                        {
                                            <table class="table table-striped table-advance table-hover">
                                                <thead>
                                                    <tr>
                                                        <th>
                                                        </th>
                                                        <th>
                                                            Nome
                                                        </th>
                                                        <th>
                                                            Faturação
                                                        </th>
                                                        <th></th>
                                                    </tr>
                                                </thead>
                                                <tbody>

                                                    @foreach (var item in Model.operadoresAveiro)
                                                    {
                                                        <tr>
                                                            <td>
                                                                @pos
                                                                @switch (pos)
                                                                {
                                                                    case 1:
                                                                        <i class="fas fa-trophy" style="color:#ff9c00"></i>
                                                                        break;
                                                                    case 2:
                                                                        <i class="fas fa-trophy" style="color:#808080"></i>
                                                                        break;
                                                                    case 3:
                                                                        <i class="fas fa-trophy" style="color:#7f3c0d"></i>
                                                                        break;
                                                                    default:
                                                                        break;
                                                                }

                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.OperadorNome)
                                                            </td>
                                                            <td>
                                                                @Html.DisplayFor(modelItem => item.Lucro)
                                                            </td>
                                                            <td>
                                                                <a class="btn btn-info btn-sm" title="Saiba mais informação sobre este funcionário" asp-area="" asp-controller="Utilizadores" asp-action="Details" asp-route-id="@item.UtilizadorId">Detalhes</a>
                                                            </td>
                                                        </tr>
                                                        pos++;
                                                    }

                                                </tbody>
                                            </table>
                                        }
                                    }
                                </section>
