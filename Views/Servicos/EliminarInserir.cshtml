
@{
    ViewData["Title"] = "Serviço Eliminado";
    Layout = "_LayoutAdmin";
}

<section id="container" class="">
    <section id="main-content">
        <section class="wrapper">
            <div class="row">
                <div class="col-lg-12">
                    <div class="card text-white bg-danger mb-3">
                        <div class="card-body">
                            <h5 class="card-title">Atenção</h5>
                            <p class="card-text">O Serviço que estava a editar foi eliminado por outra pessoa.</p>
                            <div class="form-group">
                                <input type="submit" value="Adicionar" class="btn btn-primary" />
                                <a asp-action="Index" class="btn btn-danger">Descartar</a>
                            </div>
                        </div>
                    </div>


                </div>

            </div>

        </section>
    </section>


</section>